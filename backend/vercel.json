{
    "version": 2,
    "builds": [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/",
            "methods" : [
                "GET",
                "POST",
                "PUT",
                "DELETE"
            ],
            "headers": {
                "Access-Control-Allow-Origin": "https://mern-portfolio-front-git-4a8e5b-hamzaaziz086-gmailcoms-projects.vercel.app",
                "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE"
            }
        }
    ]
}